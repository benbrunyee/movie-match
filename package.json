{
  "name": "movie-match",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "mock": "amplify mock --timeout 30 2>&1 | tee log.txt",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "build-function": "yarn amplify:movieMatchLambdaFunction",
    "amplify:movieMatchLambdaFunction": "yarn codegen && yarn yarn-function && yarn compile-function",
    "codegen": "cp -f ./src/API.ts amplify/backend/function/movieMatchLambdaFunction/lib/lib/API.ts && cp -fr ./src/graphql amplify/backend/function/movieMatchLambdaFunction/lib/lib/",
    "yarn-function": "[ -d amplify\\backend\\function\\movieMatchLambdaFunction\\src ] || mkdir amplify\\backend\\function\\movieMatchLambdaFunction\\src && cp -f amplify/backend/function/movieMatchLambdaFunction/lib/package.json amplify/backend/function/movieMatchLambdaFunction/src/package.json && cd amplify/backend/function/movieMatchLambdaFunction/src/ && yarn",
    "compile-function": "cd amplify/backend/function/movieMatchLambdaFunction && tsc -p ./tsconfig.json"
  },
  "jest": {
    "preset": "jest-expo",
    "watchPathIgnorePatterns": [
      "<rootDir>/amplify/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/amplify/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/netinfo": "7.1.3",
    "@react-native-picker/picker": "2.2.1",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "aws-amplify": "^4.3.18",
    "aws-amplify-react-native": "^6.0.4",
    "expo": "~44.0.0",
    "expo-asset": "~8.4.6",
    "expo-barcode-scanner": "~11.2.0",
    "expo-constants": "~13.0.1",
    "expo-device": "~4.1.0",
    "expo-font": "~10.0.4",
    "expo-linking": "~3.0.0",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.6",
    "expo-web-browser": "~10.1.0",
    "formik": "^2.2.9",
    "jest": "^26.6.3",
    "jest-expo": "^44.0.0",
    "lodash.debounce": "^4.0.8",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-deck-swiper": "^2.0.5",
    "react-native-qrcode-svg": "^6.1.2",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-svg": "12.1.1",
    "react-native-vision-camera": "^2.13.1",
    "react-native-web": "0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/jest": "^27.4.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-test-renderer": "^18.0.0",
    "react-test-renderer": "17",
    "typescript": "~4.3.5"
  },
  "private": true
}
